
SCRIPT_DIR=$( cd -- "$( dirname -- "${BASH_SOURCE[0]}" )" &> /dev/null && pwd )
SYMBOLS="AUDUSD EURUSD GBPUSD USDCAD USDJPY"
TIME_FRAME="H8"
for  SYMBOL in $SYMBOLS
do
    gradle run --args="excel data/${SYMBOL}-${TIME_FRAME}.csv data/${SYMBOL}-${TIME_FRAME}-reversions.csv"
    gradle run --args="plots data/${SYMBOL}-${TIME_FRAME}-reversions.csv plots"
    
done    